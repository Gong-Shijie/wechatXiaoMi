/* miniprogram/pages/notebook/notebook.wxss */

page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

.notebook {
  color: white;
  width: 95%;
  height: 200rpx;
  text-align: center;
  background-color: gray;
  margin: 20rpx;
  border-radius: 20rpx;
}

.text {
  text-align: center;
  font-size: large;
}

.time {
  font-size: small;
  color: #ddd;
  text-align: left;
  margin-left: 20rpx;
}
